{
    "name": "Throbber",
    "version": "0.0.0",
    "keywords": [
        "util",
        "functional",
        "server",
        "client",
        "browser"
    ],
    "author": "edward",
    "contributors": [],
    "dependencies": {
        "asset-assistant": "D:/Dropbox/projects-new/trunk/asset-assistant",
        "jquery": ""
    },
    "scripts": {
        "build": "npm run build-js && npm run assets",
        "build-js": "browserify src/scripts/main.js -o public_html/main.js -d",
        "assets": "assets -v && npm run inject-css",
        "inject-css": "inject public_html/*.js public_html/styles/**/*.css public_html/index.html",
        "watch-js": "watchify src/scripts/main.js -o public_html/main.js -dv",
        "clean": "assets --clean"
    },
    "assets": {
        "local-src": [
            "src/*.html"
        ],
        "src": [
            "src/styles/**/*",
            "src/assets/**/*"
        ],
        "dest": "public_html"
    },
    "main": "src/scripts/throbber/Throbber.js"
}
