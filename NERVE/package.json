{
  "name": "nerve",
  "version": "0.9.0",
  "main": "./web/js/main.js",
  "dependencies": {
    "LemmaDialog": "file:../LemmaDialog",
    "NidgetContext": "file:../NidgetContext",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify-css": "^0.14.0",
    "cd": "^0.3.3",
    "cwrc-git-dialogs": "^1.1.6",
    "cwrc-public-entity-dialogs": "^1.2.8",
    "cwrc-writer-base": "2.11.1",
    "dbpedia-entity-lookup": "^0.2.1",
    "fs": "0.0.1-security",
    "geonames-entity-lookup": "^1.1.2",
    "getty-entity-lookup": "^1.4.2",
    "jjjrmi": "^0.4.10",
    "jjjsql": "file:../../custom-lib/JJJSQL",
    "nerscriber": "file:../NERScriber",
    "nidget": "file:../Nidget",
    "rootpath": "^0.1.2",
    "viaf-entity-lookup": "^1.2.2",
    "wikidata-entity-lookup": "^1.2.2"
  },
  "devDependencies": {},
  "author": "",
  "scripts": {
    "build-js": "browserify -g [browserify-css --output web/build/main.css] web/js/main.js -o web/build/nerveapp.js -d",
    "watch-js": "watchify -g [browserify-css --output web/build/main.css] web/js/main.js -o web/build/nerveapp.js -dv",
        
    "copy-html": "copyfiles -f web/*.html web/build",
    "copy-assets-NidgetContext": "copyfiles -u 3 node_modules/NidgetContext/src/assets/**/* web/build",
    "copy-assets-LemmaDialog": "copyfiles -u 3 node_modules/LemmaDialog/src/assets/**/* web/build",
    "copy-assets-this": "copyfiles -u 1 web/assets/**/* web/build",
    "copy-assets": "npm run copy-assets-NidgetContext && npm run copy-assets-this && npm run copy-assets-LemmaDialog",
        
    "build": "npm run copy-html && npm run copy-assets && npm run build-js"
  },
  "directories": {
    "lib": "lib"
  },
  "license": "ISC",
  "description": ""
}
