    "scripts": {
        "build": "npm run build-js && npm run assets && npm run inject-css",
        "build-js": "browserify src/web/js/main.js -o web/nerveapp.js -d",
        "assets": "assets -v",
        "inject-css": "inject web/*.js web/styles/**/*.css web/nerve.html",
