    "scripts": {
        "build": "npm run build-js && npm run assets && npm run inject-css",
        "build-js": "browserify web/js/main.js -o build/nerveapp.js -d",      
        "assets": "assets -d",
        "inject-css": "inject build/styles/**/*.css build/nerve.html",

    "watch-js": "watchify web/js/main.js -o web/build/nerveapp.js -dv",
